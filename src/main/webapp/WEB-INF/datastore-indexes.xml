<?xml version="1.0" encoding="utf-8"?>
<datastore-indexes
  autoGenerate="false">
  <datastore-index kind="Reading" ancestor="true" source="manual">
    <property name="timestamp" direction="asc"/>
  </datastore-index>
  <datastore-index kind="ReadingHistory" ancestor="true" source="manual">
    <property name="timestamp" direction="asc"/>
  </datastore-index>
  <datastore-index kind="NotificationLog" ancestor="false" source="manual">
    <property name="subject" direction="asc"/>
    <property name="lastnotification" direction="desc"/>
  </datastore-index>
</datastore-indexes>
